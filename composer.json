{
  "name": "torskint/settings-editor",
  "description": "Module Laravel autonome permettant d’éditer dynamiquement le nom, email, téléphone, WhatsApp et adresse du site sans base de données",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Aristo DAKAYAO",
      "email": "da.aristo.y@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SettingsEditor\\": "src/"
    }
  },

  "scripts": {
    "post-install-cmd": [
      "@php vendor/torskint/settings-editor/message.php"
    ],
    "post-update-cmd": [
      "@php vendor/torskint/settings-editor/message.php"
    ]
  },

  "extra": {
    "laravel": {
      "providers": [
        "SettingsEditor\\SettingsEditorServiceProvider"
      ]
    }
  },

  "minimum-stability": "dev",
  "prefer-stable": true
}